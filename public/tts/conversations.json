  "negotiation": [
    {
      "id": "msg-1",
      "role": "seller",
      "text": "Hello! Welcome back. What can I prepare for you?",
      "audioPath": "/tts/negotiation_msg-1_seller.mp3",
      "filename": "negotiation_msg-1_seller.mp3"
    },
    {
      "id": "msg-2",
      "role": "customer",
      "text": "I want three portions of sate ayam, please.",
      "audioPath": "/tts/negotiation_msg-2_customer.mp3",
      "filename": "negotiation_msg-2_customer.mp3"
    },
    {
      "id": "msg-3",
      "role": "seller",
      "text": "Sate ayam, excellent choice! I don't have a price set for that item yet. What price would you suggest?",
      "audioPath": "/tts/negotiation_msg-3_seller.mp3",
      "filename": "negotiation_msg-3_seller.mp3"
    },
    {
      "id": "msg-4",
      "role": "customer",
      "text": "How about eight dollars per portion?",
      "audioPath": "/tts/negotiation_msg-4_customer.mp3",
      "filename": "negotiation_msg-4_customer.mp3"
    },
    {
      "id": "msg-5",
      "role": "seller",
      "text": "Eight dollars sounds fair! I've added three sate ayam at eight dollars each. That's twenty four dollars total. Anything else?",
      "audioPath": "/tts/negotiation_msg-5_seller.mp3",
      "filename": "negotiation_msg-5_seller.mp3",
      "orderAction": {
        "type": "add",
        "items": [
          {
            "name": "Sate Ayam",
            "quantity": 3,
            "price": 8
          }
        ]
      }
    },
    {
      "id": "msg-6",
      "role": "customer",
      "text": "Yes, add two es kopi susu.",
      "audioPath": "/tts/negotiation_msg-6_customer.mp3",
      "filename": "negotiation_msg-6_customer.mp3"
    },
    {
      "id": "msg-7",
      "role": "seller",
      "text": "Es kopi susu, great! I need a price for that as well. What would be reasonable?",
      "audioPath": "/tts/negotiation_msg-7_seller.mp3",
      "filename": "negotiation_msg-7_seller.mp3"
    },
    {
      "id": "msg-8",
      "role": "customer",
      "text": "Let's say four fifty each.",
      "audioPath": "/tts/negotiation_msg-8_customer.mp3",
      "filename": "negotiation_msg-8_customer.mp3"
    },
    {
      "id": "msg-9",
      "role": "seller",
      "text": "Perfect! Added two es kopi susu at four fifty each. Your new total is thirty three dollars. Anything more?",
      "audioPath": "/tts/negotiation_msg-9_seller.mp3",
      "filename": "negotiation_msg-9_seller.mp3",
      "orderAction": {
        "type": "add",
        "items": [
          {
            "name": "Es Kopi Susu",
            "quantity": 2,
            "price": 4.5
          }
        ]
      }
    },
    {
      "id": "msg-10",
      "role": "customer",
      "text": "That's all, thanks!",
      "audioPath": "/tts/negotiation_msg-10_customer.mp3",
      "filename": "negotiation_msg-10_customer.mp3"
    },
    {
      "id": "msg-11",
      "role": "seller",
      "text": "Excellent! Your final total is thirty three dollars.",
      "audioPath": "/tts/negotiation_msg-11_seller.mp3",
      "filename": "negotiation_msg-11_seller.mp3"
    },
    {
      "id": "msg-12",
      "role": "customer",
      "text": "Here's forty dollars cash.",
      "audioPath": "/tts/negotiation_msg-12_customer.mp3",
      "filename": "negotiation_msg-12_customer.mp3"
    },
    {
      "id": "msg-13",
      "role": "seller",
      "text": "Payment received! Your change is seven dollars. Order complete! Thank you and see you again!",
      "audioPath": "/tts/negotiation_msg-13_seller.mp3",
      "filename": "negotiation_msg-13_seller.mp3",
      "paymentReceived": {
        "amount": 40,
